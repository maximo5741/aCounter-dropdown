<?xml version='1.0' encoding='utf-8'?>
<widget id="me.mdcs.www" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>aCounter</name>
    <description>
        This is an add that was made in order to organice data.
    </description>
    <author email="maximo5741@gmail.com" href="http://cordova.io">
        Maximo Del Valle
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
