<?xml version='1.0' encoding='utf-8'?>
<widget id="me.mdcs.www" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>aCounter</name>
    <description>
        This is an add that was made in order to organice data.
    </description>
    <author email="maximo5741@gmail.com" href="http://cordova.io">
        Maximo Del Valle
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon qualifier="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon qualifier="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon qualifier="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon qualifier="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
